{
  "name": "react-intl-gettext",
  "version": "1.3.9",
  "description": "utilities to integrate react-intl with gettext",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "babel src_test -o /dev/null",
    "test": "mocha -c -u bdd --compilers js:babel-register",
    "test:watch": "watch-run -i -p src/**/*,test/**/*,!test/fixtures/messages/**/* npm run test",
    "clean": "rimraf dist/**",
    "lint": "eslint .",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel src -d dist --presets es2015,stage-0",
    "prepublish": "npm run build"
  },
  "homepage": "https://github.com/rubencosta/react-intl-gettext#readme",
  "repository": {
    "type": "git",
    "url": "github.com/rubencosta/react-intl-gettext"
  },
  "bin": {
    "rig": "dist/cli.js",
    "react-intl-gettext": "dist/cli.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react-intl",
    "gettext",
    "po",
    "pot",
    "json"
  ],
  "author": "Ruben Costa",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "expect": "^1.20.1",
    "mocha": "^2.5.3",
    "react-intl": "^2.1.2",
    "rimraf": "^2.5.2",
    "watch-run": "^1.2.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "environment": "node",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "no-extra-semi": 2
    }
  },
  "dependencies": {
    "commander": "^2.9.0",
    "gettext-parser": "^1.2.0",
    "glob": "^7.0.6"
  },
  "resolutions": {
    "commander": "2.9.0",
    "glob": "7.1.1",
    "rimraf": "2.6.1",
    "minimatch": "3.0.3",
    "once": "1.4.0",
    "lodash": "4.17.4",
    "debug": "2.6.1",
    "escape-string-regexp": "1.0.5",
    "supports-color": "2.0.0",
    "jsesc": "1.3.0",
    "ms": "0.7.2",
    "mkdirp": "0.5.1",
    "minimist": "1.2.0",
    "user-home": "2.0.0",
    "readable-stream": "2.1.5",
    "string-width": "2.0.0",
    "is-fullwidth-code-point": "2.0.0",
    "strip-json-comments": "2.0.1",
    "assert-plus": "1.0.0",
    "intl-messageformat-parser": "1.3.0",
    "doctrine": "1.5.0",
    "estraverse": "4.2.0",
    "acorn": "4.0.4"
  }
}
